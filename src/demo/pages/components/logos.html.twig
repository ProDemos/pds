{% extends 'base.html.twig' %}
{% import 'macros.html.twig' as ui %}


{% block head_title 'Logo\'s' %}


{% block body_intro %}
   <h1 >Logo's</h1>
   <p>
        Here you can find, ao, the logo's of ProDemos. <br>
        For some, The color can be changed by CSS by setting the font-color of the element.
   </p>
    <h3 >Elements and modifiers</h3>
    <ul>
        <li> .pds-c-logo</li>
    </ul>
    <br>
{% endblock %}

{% block body_content %}
    <hr>

    {% for component in components.logo %}
        {% set logo = component.logo|merge({'directory': rooturi ~ '../../assets/images/logos' }) %}
        {% set component = component|merge({'logo': logo }) %}
        {{ ui.styleguide_component(component.styleguide_type, component, styleguide.outputs) }}
    {% endfor %}
{% endblock %}
